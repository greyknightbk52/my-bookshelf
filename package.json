{
  "name": "bookshelf",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bookshelf.lol/",
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/public-sans": "^5.0.2",
    "@mui/joy": "^5.0.0-alpha.82",
    "bootstrap": "^4.6.0",
    "debounce-fn": "^4.0.0",
    "match-sorter": "^6.3.1",
    "msw": "^1.0.1",
    "node-match-path": "^0.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.9",
    "react-icons": "^4.9.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.15.0",
    "eslint-plugin-cypress": "^2.13.3",
    "husky": "^8.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.5.1",
    "netlify-plugin-cypress": "^2.2.1",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:cli": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --watch=false --coverage",
    "test:watch": "jest --watch",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test start:cli http://localhost:3000/list cy:open",
    "test:e2e:run": "start-server-and-test serve http://localhost:8811/list cy:run",
    "serve": "serve --no-clipboard --single --listen 8811 build",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
